  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start:client": "nx serve client --host=0.0.0.0 --poll=500",
    "start:signal": "nx serve signal --host=0.0.0.0",
    "start:all": "nx run-many -t serve --host=0.0.0.0 --parallel",
